{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "electron"
        ]
      }
    ],
    "linebreak-style": 0,
    "quote-props": [
      "error",
      "as-needed"
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jquery": true
  }
}